{
  "manifest_version": 3,
  "name": "Custom New Tab Page",
  "version": "1.0",
  "description": "A customizable new tab page",
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "host_permissions": [
    "chrome-extension://*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html"
}